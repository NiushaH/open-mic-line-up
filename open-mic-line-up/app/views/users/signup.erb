<h2>Sign Up for OpenMicWorld Here!</h2>
<br>
<br>
<form class="" action="/users/signup" method="post">
  <h4><label for="name">Full Name: </label></h4>
  <input type="text" name="name" placeholder="First and Last Names" value=""><br>
  <br>
  <h4><label for="cellphone">Cell Phone: </label></h4>
  <input type="text" name="cellphone" placeholder="2025551212 for example" value=""><br>
  <br>
  <h4><label for="password">Password: </label></h4>
  <input type="text" name="password" placeholder="a password you'll remember" value=""><br>
  <br>
  <br>
  <%# create user %>
  <input type="submit" name="" value="Sign Up & Create User">
</form>
<a href="/">Back</a>
